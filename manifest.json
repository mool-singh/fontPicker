{
  "manifest_version": 2,

  "name": "Font Picker",
  "description": "Check google fonts on your site.",
  "version": "1.0",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },

  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  
  "permissions": [
          "storage",
          "notifications",
          "contextMenus",
          "tabs",
          "storage",
          "activeTab",
          "<all_urls>"
        ]
}